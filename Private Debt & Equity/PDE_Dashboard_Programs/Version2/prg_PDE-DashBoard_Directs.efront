			WHERE op.CloseDate <= " & DML(%DATE) & " AND op.TYPEINVESTOP IN('EQ20')
			GROUP BY op.INVESTMENTINS
		 ) PAR_COST  ON PAR_COST.INVESTMENTINS = INVINS.IQID  


