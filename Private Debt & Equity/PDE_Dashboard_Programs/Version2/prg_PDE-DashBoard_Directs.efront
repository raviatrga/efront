		WHERE INVINS.IQDELETED=0 
		  /*AND A.IQDELETED=0 
          AND (A.CANCELLED=0 OR A.CANCELLED IS NULL)
          AND A.DRAFT=0
		  AND A.CLOSEDATE <=  " & DML(%DATE) & "
		  AND ??FILTER*/        --001
								 
	   GROUP BY  INVINS.ACCOUNTINS   
